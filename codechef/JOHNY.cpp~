#include<iostream>
#include<cstdio>
#define MAX 100005
#include<algorithm>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int a[MAX];
		int n,k;
		cin>>n;
		int i=0;
		for(i=0;i<n;i++){
			cin>>a[i];
		}
		cin>>k;
		int check=a[k-1];
		sort(a,a+n);
		int ans=0;
		for(i=0;i<n;i++){
			if(a[i]==check) break;
		}
		i++;
		cout<<i<<endl;
	}
	return 0;
}
		
		
			
	
		
		
		

