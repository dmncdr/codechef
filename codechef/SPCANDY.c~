#include<stdio.h>
int main(){
		int t;
		scanf("%d",&t);
		while(t--){
				long long int n;
				long long int k;
				scanf("%lld%lld",&n,&k);
				if(k==0)
				printf("0 %lld\n",n);
				else{
				long long int v1,v2;
				v1=n/k;
				v2=n%k;
				printf("%lld %lld\n",v1,v2);
				}}
				return 0;}
				
