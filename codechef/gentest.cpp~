#include<iostream>
#include<cstdio>
#include<math.h>
#include<cstdlib>
using namespace std;
int main(){
                        int t;
                        t=rand()%50;
                        cout<<t<<endl;
                        while(t--){
                                        int n;
                                        n=rand()%100;
                                        cout<<n<<endl;
                                        long double arr,ans=0;
                                        for(int i=0;i<n;i++){
                                        arr=-1000000000+2*(rand()%1000000000);
                                        cout<<arr<<" ";}
                                        }
                                        cout<<endl;
                                        return 0;}

